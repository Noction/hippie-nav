{
  "name": "hippie-nav",
  "version": "0.0.0",
  "main": "dist/hippie-nav.umd.js",
  "module": "dist/hippie-nav.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "mydev": "vite",
    "dev": "NODE_ENV=development vite build --watch",
    "build:light": "NODE_ENV=development vite build",
    "build": "NODE_ENV=production vite build && tsc -d --emitDeclarationOnly && vue-typegen gen -s src -o dist",
    "lint:es": "eslint \"src/**/*.{js,ts,vue}\" --fix --color",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --color --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0",
    "vue-typegen": "^0.2.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "sass": "^1.61.0"
  },
  "peerDependencies": {
    "flexsearch": "0.7.21",
    "vue-router": "^4.1.6"
  }
}
